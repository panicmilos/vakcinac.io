<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet version="2.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

  <xsl:attribute-set name="h3">
    <xsl:attribute name="text-align">center</xsl:attribute>
    <xsl:attribute name="font-size">15px</xsl:attribute>
    <xsl:attribute name="font-weight">bold</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="div">
    <xsl:attribute name="padding-bottom">8%</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="list-item">
    <xsl:attribute name="start-indent">3%</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="list-value">
    <xsl:attribute name="start-indent">5%</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="table">
    <xsl:attribute name="width">100%</xsl:attribute>
    <xsl:attribute name="border-collapse">collapse</xsl:attribute>
    <xsl:attribute name="text-align">center</xsl:attribute>
    <xsl:attribute name="border">1px solid black</xsl:attribute>
    <xsl:attribute name="margin-bottom">5%</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="th">
    <xsl:attribute name="font-weight">bold</xsl:attribute>
    <xsl:attribute name="text-align">center</xsl:attribute>
    <xsl:attribute name="border">1px solid black</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="td">
    <xsl:attribute name="text-align">center</xsl:attribute>
    <xsl:attribute name="border">1px solid black</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="bold-text">
    <xsl:attribute name="font-weight">bold</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="underline-text">
    <xsl:attribute name="text-decoration">underline</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="datum-izdavanja">
    <xsl:attribute name="padding-top">10%</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="before-table-text">
    <xsl:attribute name="padding-bottom">2%</xsl:attribute>
  </xsl:attribute-set>

  <xsl:attribute-set name="issuing-date">
    <xsl:attribute name="padding-top">10%</xsl:attribute>
  </xsl:attribute-set>

</xsl:stylesheet>